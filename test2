
zombies1 = [1, 3, 5, 7]
plants1 = [2, 4, 6, 8]
zombies2 = [1, 3,5, 7]
plants2 = [2,4]
zombies3 = [1, 3, 5, 7]
plants3= [2, 4, 0, 8]
zombies4 = [2, 1, 1, 1]
plants4 = [1, 2, 1, 1]

a = (zombies1[0] / plants1[0])
b = (zombies2[0] / plants2[0])
c = ( zombies3[0] / plants3[0])
d = (zombies4[0] / plants4[0])

max_num1 = (zombies1[0] + plants1[0] + a)/2
max_num2 = (zombies2[1] + plants2[1] + b)/2
max_num3 = (zombies3[2] + plants3[2] + c)/2
max_num4 = (zombies4[3] + plants4[3] + d)/2


if zombies1[0]  >= max_num1:
       print (True)
       
if len(zombies1) > len(plants1):
    print (True)
if len(zombies1) < len(plants1):
    print (False)
    
elif zombies1[0] == plants1[0]:
    print(False)
  
else:
       print(False)
       
       
if zombies2[1]  >= max_num2:
       print (True)
if len(zombies2) > len(plants2):
    print (True)
if len(zombies2) < len(plants2):
    print (False)       
       
elif zombies2[1] == plants2[1]:
    print(False)
   
else:
       print(False)
       

       
if zombies3[2]  >= max_num3:
       print (True)
if len(zombies3) > len(plants3):
    print (True)
if len(zombies3) < len(plants3):
    print (False)       
       
elif zombies3[2] == plants3[2]:
    print(False)
   
else:
       print(False)
         
if zombies4[3]  >= max_num4:
       print (True)
if len(zombies4) > len(plants4):
    print (True)
if len(zombies4) < len(plants4):
    print (False)       
       
elif zombies4[3] == plants4[3]:
    print(False)
   
else:
       print(False)
         
